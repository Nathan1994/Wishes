<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="wish" namespace="/wish" extends="struts-default">
        <action name="add" class="com.struts2.path.action.ShowWishesAction">
        	<result>/NewWIshEdit.jsp</result>
        </action>
        <action name="manage" class="com.struts2.path.action.ShowWishesAction">
        	<result>/MangeWishes.jsp</result>
        </action>
        <action name="more" class="com.struts2.path.action.ShowAllWishesAction">
        	<result>/MoreWishes.jsp</result>
        </action>
        <action name="pick" class="com.struts2.path.action.PickWishAction">
        	<result>/PickUp.jsp</result>
        </action>
        <action name="receive" class="com.struts2.path.action.ReceiveWishAction">
        	<result>/ShowWishes.jsp</result>
        </action>
    </package>
</struts>